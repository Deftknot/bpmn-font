{
  "name": "bpmn-font",
  "description": "A BPMN 2.0 icon font",
  "version": "0.9.1",
  "scripts": {
    "all": "run-s optimize build",
    "optimize": "svgo -f source/raw -r -o source/symbols --config '{ \"plugins\": [ { \"removeAttrs\": { \"attrs\": [ \"font-weight\", \"font-family\", \"overflow\", \"style\" ] } }] }'",
    "build": "node tasks/build.js"
  },
  "keywords": [
    "bpmn",
    "font",
    "iconfont",
    "icons"
  ],
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/nikku"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "SIL",
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/bpmn-font"
  },
  "files": [
    "dist/css/bpmn-codes.css",
    "dist/css/bpmn-embedded.css",
    "dist/css/bpmn.css",
    "dist/font/",
    "dist/demo.html",
    "resources/symbols.png",
    "LICENSE",
    "README"
  ],
  "homepage": "https://github.com/bpmn-io/bpmn-font",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "svgo": "^1.2.2",
    "tiny-glob": "^0.2.6"
  }
}
